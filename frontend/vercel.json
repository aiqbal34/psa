{
  "version": 2,
  "name": "ucdpakipsa-frontend",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://ucdpakipsa-backend-590312219539.us-central1.run.app/api/$1"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type"
        }
      ]
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://ucdpakipsa-backend-590312219539.us-central1.run.app"
  },
  "functions": {
    "src/pages/**/*.{js,ts,tsx}": {
      "maxDuration": 10
    }
  }
}